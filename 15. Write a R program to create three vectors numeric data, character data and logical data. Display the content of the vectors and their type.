a=c(1,2,3)
b=c("A","B","C")
c=c(TRUE,FALSE,TRUE,FALSE,FALSE)
print(a)
print(b)
print(c)
print(class(a))
print(class(b))
print(class(c))
#method 2
a = c(1, 2, 5, 3, 4, 0, -1, -3)
b = c("Red", "Green", "White")
c = c(TRUE, TRUE, TRUE, FALSE, TRUE, FALSE)
print(a)
print(typeof(a))
print(b)
print(typeof(b))
print(c)
print(typeof(c))
